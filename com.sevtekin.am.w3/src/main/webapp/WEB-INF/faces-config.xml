<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">
	<application>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<message-bundle>messages</message-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>testBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.TestBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>analyticsBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.AnalyticsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ownerBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.OwnerBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>clientBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.ClientBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cashFlowBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.CashFlowBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>homeBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.HomeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>siteBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.SiteBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>categoryBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.CategoryBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>importBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.ImportBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reportBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.ReportBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>snapshotBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.SnapshotBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cashDashboardBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.CashDashboardBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>categoryDashboardBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.CategoryDashboardBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ownerDashboardBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.OwnerDashboardBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>homeDashboardBean</managed-bean-name>
		<managed-bean-class>com.sevtekin.am.w3.mbean.HomeDashboardBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
        <from-view-id>/login.html</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
